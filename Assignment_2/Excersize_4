def celsius_to_fahrenheit(celsius):
    fahrenheit = (celsius * 9/5) + 32
    return fahrenheit
def fahrenheit_to_celsius(fahrenheit):
    celsius = (fahrenheit - 32) * 5/9
    return celsius
print(celsius_to_fahrenheit(22))
print(celsius_to_fahrenheit(46))
print(celsius_to_fahrenheit(51))
print(celsius_to_fahrenheit(76))
print(fahrenheit_to_celsius(22))
print(fahrenheit_to_celsius(46))
print(fahrenheit_to_celsius(51))
print(fahrenheit_to_celsius(76))
